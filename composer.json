  "type": "wp-plugin",
  "keywords": [
    "wordpress",
    "wp",
    "plugin",
    "custom post types",
    "custom taxonomies"
  ],
  "homepage": "https://github.com/digital-canvas/dc-custom-post-types/",
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Caspar Green",
      "email": "cg@caspar.green"
    }
